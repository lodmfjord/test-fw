{
  "name": "@babbstack/http-api-contract",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun build src/index.ts src/generate-contracts-bin.ts src/dev-bin.ts --outdir dist --format esm --target node --packages external && bunx tsc --project tsconfig.json --declaration --emitDeclarationOnly --declarationMap false --outDir dist --rootDir src --noEmit false"
  },
  "bin": {
    "babbstack-dev": "./dist/dev-bin.js",
    "babbstack-generate": "./dist/generate-contracts-bin.js",
    "babbstack-generator": "./dist/generate-contracts-bin.js",
    "generate-contracts-bin.js": "./dist/generate-contracts-bin.js"
  },
  "exports": {
    ".": {
      "bun": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@babbstack/dynamodb": "workspace:*",
    "@babbstack/schema": "workspace:*",
    "@babbstack/sqs": "workspace:*",
    "@babbstack/step-functions": "workspace:*",
    "esbuild": "^0.27.3",
    "zod": "^4.3.6"
  }
}
